{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["prettier", "react-app"],
    "plugins": ["prettier", "react"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error"
    }
}
